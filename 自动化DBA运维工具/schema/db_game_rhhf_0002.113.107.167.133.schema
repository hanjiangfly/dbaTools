CREATE PROCEDURE `getFirstBillPlayerid`()
BEGIN
CREATE TABLE if not exists `dba_getFirstBillPlayerid_0816` (
  `id` int(11) NOT NULL COMMENT 'id',
  `BillId` varchar(50) NOT NULL COMMENT '订单编号',
  `PlayerId` bigint(20) NOT NULL COMMENT '玩家Id',
  `GoodsId` varchar(50) NOT NULL COMMENT '货物Id',
  `ChannelGoodsId` varchar(100) DEFAULT NULL COMMENT '渠道货物Id',
  `Price` varchar(20) NOT NULL COMMENT '价格',
  `FinishTime` datetime DEFAULT NULL COMMENT '订单完成时间',
  PRIMARY KEY (`BillId`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='getFirstBillPlayerid充值订单' ;
delete from dba_getFirstBillPlayerid_0816 ;
insert into dba_getFirstBillPlayerid_0816
 SELECT zz, billid, PlayerId, GoodsId, ChannelGoodsId, price, FinishTime FROM (select i as zz, billid, PlayerId, GoodsId, ChannelGoodsId, price, FinishTime from ( SELECT @i := IF ( @playerid = playerid AND @GoodsId = GoodsId ,@i + 1, 1 ) AS i, @playerid := playerid ,@GoodsId := GoodsId, PlayerId, GoodsId, price, billid, ChannelGoodsId, FinishTime FROM t_u_bill WHERE FinishTime IS NOT NULL ORDER BY PlayerId, GoodsId, FinishTime ) AS aa ,(select @i := 0) as bb ) as a WHERE a.finishtime >= '2016-08-12 17:00:00' AND a.finishtime < '2016-08-15 21:05:00' AND a.zz = 1;
END ;;
