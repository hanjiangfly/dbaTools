CREATE PROCEDURE `get_global_unique_id`(IN id_type SMALLINT,IN id_default_value BIGINT,IN add_value INT)
BEGIN
DECLARE current_id BIGINT;
START TRANSACTION; 
SELECT `CurrentId` INTO current_id FROM `t_u_uniqueid` WHERE `IdType`=id_type FOR UPDATE;
IF current_id IS NULL THEN
SET current_id=id_default_value;
INSERT INTO `t_u_uniqueid` VALUES(id_type,current_id);
END IF;
IF current_id<id_default_value THEN
SET current_id=id_default_value;
END IF;
UPDATE `t_u_uniqueid` SET `CurrentId`=current_id+add_value WHERE `IdType`=id_type;
COMMIT;
SELECT current_id;
END ;;
DELIMITER ;

