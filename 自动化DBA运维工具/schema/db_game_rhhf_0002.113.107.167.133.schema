CREATE PROCEDURE `update_bill_finish_time`(IN i_billId VARCHAR(50),IN i_finishTime DATETIME,IN i_serial_number VARCHAR(50),IN i_channel_bill_no VARCHAR(128),IN i_paychannel VARCHAR(50))
BEGIN
        DECLARE v_oldFinishTime DATETIME;
        DECLARE v_result INT;
        SET v_result = 0;
        START TRANSACTION; 
        SELECT `FinishTime` INTO v_oldFinishTime FROM `t_u_bill` WHERE `BillId`=i_billId FOR UPDATE;
        IF v_oldFinishTime IS NULL THEN
            UPDATE `t_u_bill` SET `FinishTime`=i_finishTime,`SerialNumber`=i_serial_number,`ChannelBillNo`=i_channel_bill_no,`PayChannel`= i_paychannel WHERE `BillId`=i_billId;
            SET v_result = 1;
        END IF;
        COMMIT;
        SELECT v_result;   
END ;;
DELIMITER ;

